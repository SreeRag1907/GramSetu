{
  "expo": {
    "name": "GramSetu",
    "slug": "gramsetu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gramsetu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "network_security_config",
      "package": "com.gramsetu.app",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow GramSetu to use your location to provide local weather and market information."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "flaskApiUrl": "http://192.168.1.7:5000",
      "router": {},
      "eas": {
        "projectId": "f192bbaf-ef10-4850-ad0b-ec35167ce31c"
      }
    }
  }
}
